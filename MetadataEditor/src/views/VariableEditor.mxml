<?xml version="1.0" encoding="utf-8"?>
<f:Slider xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:f="library://ns.feathersui.com/mxml"
		  xmlns:renderers="views.editor.renderers.*">
	<fx:Script>
		<![CDATA[
			
			private var _data:Object;

			public function get data():Object
			{
				return _data;
			}

			public function set data(value:Object):void
			{
				_data = value;
				if (cr)
					cr.data = value;
			}

		]]>
	</fx:Script>
	
	<f:layoutData>
		<f:VerticalLayoutData percentHeight="100" percentWidth="100"/>
	</f:layoutData>
	
	<renderers:ClassRenderer id="cr" data="{data}">
		
	</renderers:ClassRenderer>
</f:Slider>
